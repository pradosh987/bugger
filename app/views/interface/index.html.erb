      <!-- 
       Contact us
       ====================================== -->
       <div class="highlight">
           <div class="container">

               <div class="row">
                   <div class="col-sm-12">
                       <div class="bugger-progress">
                          
                       </div>
                   </div>
               </div>
               <!--  // end .row -->
           </div>
       </div>

      <div class="highlight result-container" ng-repeat="item in results">
          <div class="container">

              <div class="row">
                  <div class="col-sm-12">
                      <div class="contact-box bugger-result">
                          <div class="result" ng-click="toggle-errors()" >
                            <div class="overview">
                              <div class="preview-image">
                                <img src="http://btesimages.s3.amazonaws.com/Wolfpack/wp_dontpassthebuck_orange_1.jpg" class="img-thumbnail">
                              </div>
                              
                              <div class="title">
                                <p>{{item.title}}</p>
                                <small>{{item.sku_id}}</small>
                              </div>

                              <div class="status">
                                <span class="glyphicon glyphicon-ok-circle" aria-hidden="true" ng-if="item.passed"></span>
                                <span class="glyphicon glyphicon-remove-circle" aria-hidden="true" ng-if="!item.passed"></span>
                              </div>
                            </div>
                          </div>
                          <div class="bugger-errors" ng-if="!item.passed" id="{$index}">
                            <div class="bs-example">
                              <table class="table table-hover"> 
                                <thead> 
                                  <tr> 
                                    <th>#</th>
                                    <th>Field</th> 
                                    <th>Expected</th> 
                                    <th>Actual</th>
                                  </tr> 
                                </thead> 
                                <!-- Errors -->
                                <tbody ng-repeat="error in item.errors"> 
                                  <tr> 
                                    <th scope="row">1</th> 
                                    <td>{{error.field}}</td> 
                                    <td>{{error.expected}}</td> 
                                    <td>{{error.actual}}</td> 
                                  </tr>  
                                </tbody> 
                              </table>
                            </div>
                          </div>
                      </div>
                  </div>
                  
              </div>
              <!--  // end .row -->
          </div>
      </div>
      <!-- //  end .highlight -->

